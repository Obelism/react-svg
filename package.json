{
	"name": "@obelism/react-svg",
	"description": "A performant way to load and show SVG in React applications",
	"keywords": [
		"svg",
		"react",
		"icons",
		"request",
		"cache"
	],
	"license": "MIT",
	"version": "2.1.3",
	"author": "Obelism <hello@obelism.studio> (https://www.obelism.studio)",
	"repository": {
		"type": "git",
		"url": "https://github.com/Obelism/react-svg"
	},
	"main": "./dist/index.cjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "bunchee -w",
		"build": "bunchee",
		"test": "vitest",
		"test:ci": "vitest run",
		"lint": "biome check",
		"lint:fix": "biome check --fix",
		"format": "biome format --write"
	},
	"peerDependencies": {
		"react": ">=17.0.0",
		"swr": ">=2.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^22.19.1",
		"@types/react": "^19.2.7",
		"@vitejs/plugin-react": "^5.1.1",
		"bunchee": "^6.6.2",
		"jsdom": "^27.2.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"typescript": "^5.0.4",
		"vitest": "^4.0.14"
	}
}
